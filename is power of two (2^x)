#include <iostream>

using namespace std;

bool isPowerOfTwo(int number){
    if(number == 0) return false;
    
    return (number & (number- 1)) == 0;
}

int main()
{
    
    int num = 20;
    bool check = isPowerOfTwo(num);
    if(check) cout<<"Power of two";
    else cout<<"Not power of two"; 
}
